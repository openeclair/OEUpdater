<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background"
    android:gravity="center">
    <LinearLayout
	    android:orientation="horizontal"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content">
	    <TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/download_info_filename"
			android:layout_gravity="left"/>
		<TextView
			android:layout_width="0dip"
			android:layout_height="wrap_content"
			android:layout_weight="1"/>
	    <TextView
			android:id="@+id/filename_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="right"/>
	</LinearLayout>
	<LinearLayout
	    android:orientation="horizontal"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content">
	    <TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/download_info_mirror"
			android:layout_gravity="left"/>
		<TextView
			android:layout_width="0dip"
			android:layout_height="wrap_content"
			android:layout_weight="1"/>
	    <TextView
			android:id="@+id/mirror_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="right"/>
	</LinearLayout>
    <LinearLayout
	    android:orientation="horizontal"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    >	   
    	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/download_info_downloading"
			android:layout_weight="0"
			android:layout_gravity="left"/>
		<TextView
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:layout_weight="1"/>
		<TextView
			android:id="@+id/bytes_downloaded_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="right"
			android:layout_weight="0"/>
    </LinearLayout>
	<ProgressBar
		android:id="@+id/download_progress_bar"
		style="?android:attr/progressBarStyleHorizontal"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" />
	<LinearLayout
	    android:orientation="horizontal"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    >	   
    	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/download_info_speed"
			android:layout_weight="0"
			android:layout_gravity="left"/>
		<TextView
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:layout_weight="1"/>
		<TextView
			android:id="@+id/speed_text_view"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="right"
			android:layout_weight="0"/>
    </LinearLayout>
	<LinearLayout
	    android:orientation="horizontal"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content">
    	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/download_info_remaining_time"
			android:layout_weight="0"
			android:layout_gravity="left"/>
		<TextView
			android:layout_width="fill_parent"
